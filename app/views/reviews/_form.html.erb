<%= form_for([@course, @course.reviews.build]) do |f| %>
    <p> 
       <%= f.label :rating %> <br>
       <%= f.number_field :rating %>
    </p>

    <p> 
    <%= f.label :grade %> <br>
       <%= f.text_field :grade %>
    </p>
   
    <p> 
       <%= f.label :info %> <br>
       <%= f.text_field :info %>
    </p>
   
    <p> 
       <%= f.label :review_text %> <br>
       <%= f.text_area :review_text %>
    </p>

    <br>
    <p> <%= f.submit %> </p>
<% end %>
